@using nugsnet6.Models
@using CodeMechanic.Diagnostics
@model nugsnet6.Models.FakeUser

<label class="label w-100">nameof(Role)</label>

@Html.DropDownListFor(user => user.Role,
    new SelectList(
        Role.GetAll<Role>()
            .Dump("options 1")
            .Select(x => x.Name)
            .ToArray()
            .Dump("options")
        ,
        "Select Role"))